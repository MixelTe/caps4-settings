{
  "name": "caps4-settings",
  "displayName": "Caps4 Settings",
  "description": "Syntax highlighting for Caps4 settings",
  "icon": "imgs/icon.png",
  "version": "1.1.0",
  "publisher": "MixelTe",
  "repository": {
    "url": "https://github.com/MixelTe/caps4-settings",
    "type": "git"
  },
  "engines": {
    "vscode": "^1.94.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "caps4",
      "aliases": ["Caps4", "caps4"],
      "extensions": [".caps-settings"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "caps4",
      "scopeName": "source.caps4",
      "path": "./syntaxes/caps4.tmLanguage.json",
      "embeddedLanguages": {
        "meta.embedded.block.javascript": "javascript"
      }
    }]
  }
}
